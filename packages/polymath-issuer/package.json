    "react-select": "^2.0.0",
    "react-transition-group": "^2.2.1",
    "redux": "3.7.2",
    "redux-form": "7.0.4",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "svgr": "1.9.2",
